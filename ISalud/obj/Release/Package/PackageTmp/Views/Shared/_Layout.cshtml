<script>
    $(document).ajaxError(function (xhr, props) {
        if (props.status === 405) {
            swal({
                title: 'Atención',
                text: "La sesión ha expirado.",
                type: 'warning',
                buttonsStyling: true,
                confirmButtonColor: "#FF0000"
            }).then(() => {
                window.location.href = 'Login';
            });
        }
    });
</script>